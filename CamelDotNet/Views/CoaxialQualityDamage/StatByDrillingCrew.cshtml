@model List<DotNet.Highcharts.Highcharts>
@if (ViewBag.ErrorMsg != null)
{
    <div>
        @Html.Raw(ViewBag.ErrorMsg)
    </div>
}

@{
    decimal totalFailDamage = Convert.ToDecimal(ViewBag.TotalDamage);
}

<div>
    @foreach (var chart in Model)
    {
        @chart
    }
</div>

<div>
    <span><b>损失合计：</b>@string.Format("{0:0.#####}", totalFailDamage)</span>
</div>

